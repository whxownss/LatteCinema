<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Cinema">

	<select id="select" resultType="CinemaDTO">
		SELECT LO_IDX
			 , CI_IDX
			 , CI_NAME
			 , CI_OC
			 , CI_ADDR
			 , CI_S_NUM
		  FROM CINEMA
		 WHERE 1=1
	</select>

